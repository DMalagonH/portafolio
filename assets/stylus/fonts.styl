		url('../fonts/icomoon.woff?5hrfeu') format('woff'),
		url('../fonts/icomoon.ttf?5hrfeu') format('truetype'),
		url('../fonts/icomoon.svg?5hrfeu#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
